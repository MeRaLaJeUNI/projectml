<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <!-- Puedes definir variables aquÃ­ si es necesario -->
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="16dp">

        <TextView
            android:id="@+id/textViewDatos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Datos predefinidos"
            android:textSize="18sp" />

        <Button
            android:id="@+id/buttonAgregarPropiedades"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Agregar Propiedades" />

        <Button
            android:id="@+id/buttonMostrarPropiedades"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Mostrar Propiedades" />

    </LinearLayout>
</layout>
